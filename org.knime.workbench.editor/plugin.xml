<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="metanode_porttype" name="Port Type for Meta Node Wizard" schema="schema/metanode_portype.exsd"/>
     <extension
         point="org.eclipse.ui.commands">
      <category
            description="Commands available for the workflow editor"
            name="KNIME Workflow Editor commands"
            id="knime.command.category"/>
            
      <command
            categoryId="knime.command.category"
            description="Opens the configuration dialog on the selected node"
            id="knime.commands.openDialog"
            name="Configure..."/>
      <command
            categoryId="knime.command.category"
            description="Execute workflow up to here"
            id="knime.commands.execute"
            name="Execute"/>
      <command
            categoryId="knime.command.category"
            description="Execute all"
            id="knime.commands.executeall"
            name="Execute All"/>
      <command
            categoryId="knime.command.category"
            description="Cancel"
            id="knime.commands.cancel"
            name="Cancel"/>
      <command
            categoryId="knime.command.category"
            description="Cancel all"
            id="knime.commands.cancelall"
            name="Cancel All"/>  
      <command
            categoryId="knime.command.category"
            description="Reset node"
            id="knime.commands.reset"
            name="Reset"/>
      <command
            categoryId="knime.command.category"
            description="Hide node names"
            id="knime.commands.hideNodeNames"
            name="Hide Node Names"/>
      <command
            categoryId="knime.command.category"
            description="Open first view for selected nodes"
            id="knime.commands.openView"
            name="Open View"/>
      <command
            categoryId="knime.command.category"
            description="Execute all selected nodes and open first view"
            id="knime.commands.executeAndOpenView"
            name="Execute and Open View"/>
      <command
            categoryId="knime.command.category"
            description="Edit custom description for the selected node"
            id="knime.commands.setNameAndDescription"
            name="Edit Node Description..."/>
      <command
            categoryId="knime.command.category"
            description="Open first out-port view for selected nodes"
            id="knime.commands.openFirstOutPortView"
            name="Open First Out-port View"/>
      <command
            categoryId="knime.command.category"
            description="Open wizard to add meta node"
            id="knime.commands.addMetaNode"
            name="Open Meta Node Wizard..."/>
      <command
            categoryId="knime.command.category"
            description="Checks and updates meta node links"
            id="knime.commands.updateMetaNodeLink"
            name="Update meta node link"/>
      <command
            categoryId="knime.command.category"
            description="Do one loop step"
            id="knime.commands.steploop"
            name="Do one loop step"/>
      <command
            categoryId="knime.command.category"
            description="Pause loop execution"
            id="knime.commands.pauseexecution"
            name="Pause loop execution"/>  
      <command
            categoryId="knime.command.category"
            description="Resume loop execution"
            id="knime.commands.resumeloop"
            name="Resume loop execution"/>       
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.knime.workbench.editor2.WorkflowEditor"
            contributorClass="org.knime.workbench.editor2.WorkflowEditorActionBarContributor"
            default="true"
            extensions="knime"
            filenames="knime"
            icon="icons/knime.png"
            id="org.knime.workbench.editor.WorkflowEditor"
            name="KNIME Workflow Editor"/>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.knime.workbench.editor.editorContribution"
            targetID="org.knime.workbench.editor.WorkflowEditor">
         <menu
               id="org.knime.workbench.editor.menu"
               label="Node"
               path="additions">
            <groupMarker name="group1"/>
         </menu>
         <action
               class="org.knime.workbench.editor2.actions.delegates.ResumeLoopEditorAction"
               definitionId="knime.commands.resumeloop"
               disabledIcon="icons/resume_disabled.PNG"
               icon="icons/resume.png"
               id="org.knime.workbench.editor.actions.resumeloop"
               label="Resume Loop Execution"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               tooltip="Resume loop execution"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.PauseLoopExecutionEditorAction"
               definitionId="knime.commands.pauseexecution"
               disabledIcon="icons/pause_disabled.png"
               icon="icons/pause.png"
               id="org.knime.workbench.editor.actions.pauseexecution"
               label="Pause Execution"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               tooltip="Pause execution"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.StepLoopEditorAction"
               definitionId="knime.commands.steploop"
               disabledIcon="icons/step_disabled.png"
               icon="icons/step.png"
               id="org.knime.workbench.editor.actions.stepLoop"
               label="Step Loop Execution"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               tooltip="Do one loop step"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.HideNodeNamesEditorAction"
               definitionId="knime.commands.hideNodeNames"
               disabledIcon="icons/hideNodeNames_disabled.png"
               icon="icons/hideNodeNames.png"
               id="org.knime.workbench.editor.actions.hideNodeNames"
               label="Hide Node Names"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="toggle"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Hide all node names"/>
         <action
               class="org.knime.workbench.editor2.actions.AddMetaNodeAction"
               definitionId="knime.commands.addMetaNode"
               disabledIcon="icons/meta/metanode_add.png"
               icon="icons/meta/metanode_add.png"
               id="org.knime.workbench.editor.meta"
               label="Open Meta Node Wizard..."
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Opens the Add Meta Node Wizard"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.CheckUpdateMetaNodeLinkAllEditorAction"    
               definitionId="knime.commands.updateMetaNodeLink"
               disabledIcon="icons/meta/metanode_link_update.png"
               icon="icons/meta/metanode_link_update.png"
               id="knime.action.meta_node_check_update_link_all"
               label="Update Meta Node Links"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               tooltip="Check and update meta node links"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.DefaultOpenViewEditorAction"
               definitionId="knime.commands.openView"
               disabledIcon="icons/openView_disabled.gif"
               icon="icons/openView.gif"
               id="org.knime.workbench.editor.actions.defaultOpen"
               label="Open First View"
               menubarPath="org.knime.workbench.editor.menu/group1"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Opens the first view of the selected nodes"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.OpenFirstOutPortViewEditorAction"
               definitionId="knime.commands.openFirstOutPortView"
               disabledIcon="icons/openPortView_disabled.png"
               icon="icons/openPortView.png"
               id="org.knime.workbench.editor.actions.openFirstOutPortView"
               label="Open First Out-Port View"
               menubarPath="org.knime.workbench.editor.menu/group1"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Opens the first out-port view of the selected nodes"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.SetNodeDescriptionEditorAction"
               definitionId="knime.commands.setNameAndDescription"
               disabledIcon="icons/setNameDescription_disabled.PNG"
               icon="icons/setNameDescription.PNG"
               id="org.knime.workbench.editor.actions.setnameanddescription"
               label="Edit Node Name and Description..."
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Edit custom node name and node description"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.ResetEditorAction"
               definitionId="knime.commands.reset"
               disabledIcon="icons/resetNode_disabled.gif"
               icon="icons/resetNode.gif"
               id="org.knime.workbench.editor.actions.reset"
               label="Reset"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Reset selected nodes"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.CancelAllEditorAction"
               definitionId="knime.commands.cancelall"
               disabledIcon="icons/cancelAll_disabled.PNG"
               icon="icons/cancelAll.PNG"
               id="org.knime.workbench.editor.actions.cancelall"
               label="Cancel All"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Cancel all running nodes"/>     
         <action
               class="org.knime.workbench.editor2.actions.delegates.CancelEditorAction"
               definitionId="knime.commands.cancel"
               disabledIcon="icons/cancel_disabled.PNG"
               icon="icons/cancel.PNG"
               id="org.knime.workbench.editor.actions.cancel"
               label="Cancel"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Cancel all selected, running nodes"/>                   
         <action
               class="org.knime.workbench.editor2.actions.delegates.ExecuteAndOpenViewEditorAction"
               definitionId="knime.commands.executeAndOpenView"
               disabledIcon="icons/executeAndView_disabled.PNG"
               icon="icons/executeAndView.PNG"
               id="org.knime.workbench.editor.actions.executeAndOpenView"
               label="Execute and Open Views"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Execute selected nodes and open first view"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.ExecuteAllEditorAction"
               definitionId="knime.commands.executeall"
               disabledIcon="icons/executeAll_disabled.PNG"
               icon="icons/executeAll.PNG"
               id="org.knime.workbench.editor.actions.executeall"
               label="Execute All"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Execute all executable nodes"/>
         <action
               class="org.knime.workbench.editor2.actions.delegates.ExecuteEditorAction"
               definitionId="knime.commands.execute"
               disabledIcon="icons/execute_disabled.PNG"
               icon="icons/execute.PNG"
               id="org.knime.workbench.editor.actions.execute"
               label="Execute"
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Execute selected and executable nodes"/>    
         <action
               class="org.knime.workbench.editor2.actions.delegates.OpenDialogEditorAction"
               definitionId="knime.commands.openDialog"
               disabledIcon="icons/openDialog_disabled.gif"
               icon="icons/openDialog.gif"
               id="org.knime.workbench.editor.actions.openDialog"
               label="Configure..."
               menubarPath="org.knime.workbench.editor.menu/group1"
               style="push"
               toolbarPath="org.knime.workbench.editor.menu/group1"
               tooltip="Configure selected node"/>
      </editorContribution>
   </extension>
   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="knime.commands.addMetaNode"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+F12"/>
      <key
            commandId="knime.commands.updateMetaNodeLink"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+ALT+U"/>
      <key
            commandId="knime.commands.setNameAndDescription"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ALT+F2"/>
      <key
            commandId="knime.commands.openDialog"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F6"/>
      <key
            commandId="knime.commands.openView"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F10"/>
      <key
            commandId="knime.commands.executeAndOpenView"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+F10"/>
      <key
            commandId="knime.commands.reset"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F8"/>
      <key
            commandId="knime.commands.hideNodeNames"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+ALT+Q"/>    
      <key
            commandId="knime.commands.execute"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F7"/>
      <key
            commandId="knime.commands.executeall"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+F7"/>
      <key
            commandId="knime.commands.cancel"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F9"/>
      <key
            commandId="knime.commands.cancelall"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+F9"/>
      <key
            commandId="knime.commands.openFirstOutPortView"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+F6"/>
      <key
            commandId="knime.commands.steploop"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ALT+CTRL+F6"/>
      <key
            commandId="knime.commands.pauseexecution"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ALT+CTRL+F7"/>
      <key
            commandId="knime.commands.resumeloop"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="ALT+CTRL+F8"/>  
   </extension>
    <extension
         id="modelProvider"
         name="Rename avoidance"
         point="org.eclipse.core.resources.modelProviders">
      <modelProvider
            class="org.knime.workbench.editor2.MoveModelProvider"
       />
      <extends-model id="org.knime.workbench.editor2.MoveModelProvider"/>
      <enablement>
			<instanceof value="org.eclipse.core.resources.IResource"/>
      </enablement>
   </extension>
    <extension
          point="org.knime.workbench.editor.metanode_porttype">
       <porttype
             metaNodePortTypeClass="org.knime.workbench.editor2.meta.MetaNodePortType$DataMetaNodePortType">
       </porttype>
       <porttype
             metaNodePortTypeClass="org.knime.workbench.editor2.meta.MetaNodePortType$DBMetaNodePortType">
       </porttype>
       <porttype
             metaNodePortTypeClass="org.knime.workbench.editor2.meta.MetaNodePortType$PMMLMetaNodePortType"></porttype>
       <porttype
             metaNodePortTypeClass="org.knime.workbench.editor2.meta.MetaNodePortType$FlowVarMetaNodePortType">
       </porttype>
    </extension>
     <extension
         point="org.eclipse.ui.views">
      <view
            category="org.knime.workbench.ui.category"
            class="org.knime.workbench.helpview.HelpView"
            icon="icons/node_description.png"
            id="org.knime.workbench.helpview"
            name="Node Description"/>
   </extension>
</plugin>
