<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.knime.workbench.editor.svgexport.actionSet"
            label="Export to SVG"
            visible="true">
         <action
               class="org.knime.workbench.editor.svgexport.actions.ExportToSVGAction"
               icon="icons/export.png"
               id="ExportToSVGAction"
               label="Export to SVG..."
               menubarPath="file/ExportWorkflow"
               tooltip="Export the current workflow as an SVG-image">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.knime.core.ui.util.AWTWorkflowRenderer">
      <AWTWorkflowRenderer
            AWTWorkflowRenderer="org.knime.workbench.editor.svgexport.AWTWorkflowRendererImpl">
      </AWTWorkflowRenderer>
   </extension>

</fragment>
